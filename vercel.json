{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.7.4"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "buildCommand": "chmod +x build.sh && ./build.sh",
    "installCommand": "composer install --no-dev --optimize-autoloader",
    "framework": "laravel"
  }
